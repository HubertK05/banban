{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "banban",
  "mainBinaryName": "banban",
  "version": "0.0.1",
  "identifier": "com.banban.dev",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Banban",
        "fullscreen": false,
        "resizable": true,
        "width": 1280,
        "height": 800,
        "visible": false,
        "useHttpsScheme": true
      },
      {
        "fullscreen": false,
        "resizable": true,
        "width": 1280,
        "height": 800,
        "decorations": false,
        "url": "splashscreen.html",
        "label": "splashscreen",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}